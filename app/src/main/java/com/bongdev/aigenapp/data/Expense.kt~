package com.bongdev.aigenapp.data

import java.util.Date

data class Expense(
    val id: Long = 0,
    val amount: Double,
    val category: String,
    val date: Date = Date(),
    val note: String? = null
)

enum class ExpenseCategory {
    FOOD,
    TRANSPORTATION,
    ENTERTAINMENT,
    SHOPPING,
    BILLS,
    HEALTH,
    EDUCATION,
    OTHER
} 